<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="http-response_title"
          extraClientlibs="httperrors"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <tab1
                            jcr:primaryType="nt:unstructured"
                            jcr:title="http-tab_name"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items
                                        jcr:primaryType="nt:unstructured">
                                    <well jcr:primaryType="nt:unstructured"
                                          sling:resourceType="granite/ui/components/foundation/container">
                                        <layout jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <responseTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="http-response_title"
                                                    name="./httpErrorTittle"/>
                                            <responseDescription
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                    fieldLabel="g08_field_firstdescription"
                                                    fieldDescription="http-response_description"
                                                    name="./httpErrorDescription"
                                                    useFixedInlineToolbar="{Boolean}true"/>
                                        </items>
                                    </well>
                                </items>
                            </columns>
                        </items>
                    </tab1>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>